{
  "name": "sakura",
  "version": "1.0.0",
  "description": "Code that does things...",
  "license": "UNLICENSED",
  "author": "Kevin",
  "scripts": {
    "start": "node ./src/index.js",
    "update": "yarn update",
    "test": "eslint . silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkiraKokiri/Sakura.git"
  },
  "bugs": {
    "url": "https://github.com/AkiraKokiri/Sakura/issues"
  },
  "dependencies": {
    "@discordjs/opus": "0.5.0",
    "@naval-base/ms": "3.1.0",
    "@sentry/node": "6.2.3",
    "@sentry/tracing": "6.2.3",
    "axios": "0.21.1",
    "chalk": "4.1.0",
    "common-tags": "1.8.0",
    "discord-akairo": "github:discord-akairo/discord-akairo",
    "discord.js": "github:discordjs/discord.js",
    "dotenv": "8.2.0",
    "emoji-regex": "9.2.2",
    "erlpack": "github:hammerandchisel/erlpack",
    "google-tts-api": "2.0.2",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "moment-duration-format": "2.3.2",
    "pg": "8.5.1",
    "sequelize": "6.6.1"
  },
  "devDependencies": {
    "eslint": "7.22.0",
    "eslint-config-aqua": "8.1.2",
    "nodemon": "2.0.7"
  },
  "eslintConfig": {
    "extends": "aqua",
    "rules": {
      "es6": "true",
      "no-process-env": "off",
      "comma-dangle": "off",
      "quote-props": "off",
      "no-use-before-define": "off",
      "max-len": "off",
      "eol-last": "off",
      "no-console": "off",
      "no-inline-comments": "off",
      "capitalized-comments": "off",
      "no-undef": "off",
      "max-depth": "warn",
      "curly": "off",
      "nonblock-statement-body-position": "off",
      "indent": "off",
      "prefer-destructuring": [
        "warn"
      ]
    }
  },
  "engines": {
    "node": ">=14"
  }
}
