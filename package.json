{
  "name": "sakura",
  "version": "1.0.0",
  "description": "Sakura - The Discord Bot",
  "license": "MIT",
  "author": "Kevin",
  "scripts": {
    "start": "node ./src/index.js",
    "update": "yarn update",
    "test": "eslint . silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkiraKokiri/Rei.git"
  },
  "bugs": {
    "url": "https://github.com/AkiraKokiri/Rei/issues"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "@naval-base/ms": "^3.1.0",
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "common-tags": "^1.8.0",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.3.1",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.0.0",
    "erlpack": "github:hammerandchisel/erlpack",
    "google-tts-api": "^0.0.4",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "moment-duration-format": "^2.3.2",
    "pg": "^8.3.3",
    "raven": "^2.6.4",
    "sequelize": "^5.8.7"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-aqua": "^7.3.0",
    "nodemon": "^2.0.4"
  },
  "eslintConfig": {
    "extends": "aqua",
    "rules": {
      "es6": "true",
      "no-process-env": "off",
      "comma-dangle": "off",
      "quote-props": "off",
      "no-use-before-define": "off",
      "max-len": "off",
      "eol-last": "off",
      "no-console": "off",
      "no-inline-comments": "off",
      "capitalized-comments": "off",
      "no-undef": "off",
      "max-depth": "warn",
      "curly": "off",
      "nonblock-statement-body-position": "off",
      "indent": "off",
      "prefer-destructuring": [
        "warn"
      ]
    }
  },
  "engines": {
    "node": ">=12"
  }
}
